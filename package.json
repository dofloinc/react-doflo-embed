{
  "name": "react-doflo-embed",
  "version": "0.0.0-development",
  "description": "React component to aid in the embedding of doFlo Elements",
  "main": "dist/cjs/react-doflo-embed.js",
  "module": "dist/esm/react-doflo-embed.js",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "scripts": {
    "test": "echo 'no test specified'",
    "preversion": "npm test",
    "build": "rm -rf ./dist && echo {'\"'version'\"':$(jq '.version' ./package.json)}>./src/version.json && NODE_ENV=production rollup -c",
    "prepack": "echo {'\"'version'\"':$(jq '.version' ./package.json)}>./dist/version.json",
    "postversion": "npm run build",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "doflo",
    "resize"
  ],
  "author": {
    "name": "doFlo Team",
    "email": "developers@doflo.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dofloinc/react-doflo-embed.git"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^18.0.0",
    "react-dom": "^15.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@semantic-release/npm": "^9.0.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "husky": "^7.0.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0"
  }
}
